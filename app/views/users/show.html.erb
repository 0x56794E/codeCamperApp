<div class="prof-cards-container">
<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <div class="prof-card">
      <img class="prof-user-image" src="<%=@user.image%>" />
      <h1 class = "prof-user-name">
        <%= @user.name %>
      </h1>

        <div class = "prof-user-email">Contact Me: <%= @user.email %></div>
        <div class = "prof-user-location">Location: <%= @user.location %></div>
        <div class = "prof-user_school">School: <%= @user.school %></div>
        <div class = "prof-user-gen">Generation: <%= @user.generation %></div>
      </div>
    </section>
  </aside>
</div>
</div>

